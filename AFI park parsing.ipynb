    "# импортируем необходимые библиотеки, при необходимости устанавливаем \n",
    "import requests\n",
    "import pandas as pd\n",
    "import gspread\n",
    "import psycopg2\n",
    "from fake_useragent import UserAgent\n",
    "import random\n",
    "import pprint\n",
    "from datetime import datetime\n",
    "\n",
    "pd.set_option('display.max_columns', None)"
   ]
